<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <!-- Skip jOOQ generated code. -->
  <Match>
    <Package name="keywhiz.jooq"/>
  </Match>
  <Match>
    <Package name="keywhiz.jooq.tables"/>
  </Match>

  <!-- Not interested in serializing a servlet. -->
  <Match>
    <Class name="keywhiz.FileAssetServlet"/>
    <Bug code="SnVI"/>
  </Match>
</FindBugsFilter>
